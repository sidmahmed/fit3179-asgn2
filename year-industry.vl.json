{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 400,
  "description": "Stock prices of 5 Tech Companies over Time.",
  "data": {
    "url": "https://raw.githubusercontent.com/ahmedbonyeamin/fit3179-asgn2/master/cleaned%20data/year-industry.csv"
  },
  "mark": {"type": "line", "point": true},
  "encoding": {
    "x": {"timeUnit": "year", "field": "Year"},
    "y": {
      "field": "Number",
      "type": "quantitative",
      "title": "Number of Visas Granted"
    },
    "color": {"field": "Industry", "type": "nominal"}
  },
  "params": [
    {
      "name": "Industry_selection",
      "bind": {
        "input": "select",
        "options": [
          null,
          "Accommodation and Food Services",
          "Administrative and Support Services",
          "Agriculture, Forestry and Fishing",
          "Arts and Recreation Services",
          "Construction",
          "Education and Training",
          "Electricity, Gas, Water and Waste Services",
          "Financial and Insurance Services",
          "Health Care and Social Assistance",
          "Information Media and Telecommunications",
          "Manufacturing",
          "Mining",
          "Other Services",
          "Professional, Scientific and Technical Services",
          "Public Administration and Safety",
          "Rental, Hiring and Real Estate Services",
          "Retail Trade",
          "Transport Postal and Warehousing",
          "Wholesale Trade"
        ],
        "labels": [
          "Show All",
          "Accommodation and Food Services",
          "Administrative and Support Services",
          "Agriculture, Forestry and Fishing",
          "Arts and Recreation Services",
          "Construction",
          "Education and Training",
          "Electricity, Gas, Water and Waste Services",
          "Financial and Insurance Services",
          "Health Care and Social Assistance",
          "Information Media and Telecommunications",
          "Manufacturing",
          "Mining",
          "Other Services",
          "Professional, Scientific and Technical Services",
          "Public Administration and Safety",
          "Rental, Hiring and Real Estate Services",
          "Retail Trade",
          "Transport Postal and Warehousing",
          "Wholesale Trade"
        ],
        "name": "Industry selection: "
      }
    },
    {
      "name": "Year",
      "value": 2019,
      "bind": {
        "input": "range",
        "min": 2007,
        "max": 2019,
        "step": 1,
        "name": "Year"
      }
    }
  ],
  "transform": [
    {
      "filter": "Industry_selection == null || datum.Industry == Industry_selection"
    },
    {"filter": {"timeUnit": "year", "field": "Year", "lte": "Year"}}
  ],
  "config": {}
}