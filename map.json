{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Australian Immigrant Population",
  "width": 800,
  "height": 600,
  "projection": {"type": "equirectangular"},
  "params": [
  {
    "name": "Year_selection",
    "value": 2016,
    "bind": {"input": "select", "options": ["1996", "2001", "2006", "2011", "2016"]}
    }
],
  "data": {
    "url": "https://raw.githubusercontent.com/ahmedbonyeamin/fit3179-asgn2/master/cleaned%20data/state-resident.csv"
  },
  "transform": [
    {
      "lookup": "State",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/ahmedbonyeamin/fit3179-asgn2/master/cleaned%20data/STE_2016_AUST.json",
          "format": {"type": "topojson", "feature": "STE_2016_AUST"}
        },
        "key": "properties.STE_NAME16"
      },
      "as": "geo"
    },
    {"filter": "datum.Year == Year_selection"}
  ],
  "mark": {"type": "geoshape"},
  "encoding": {
    "shape": {
        "field": "geo",
        "type": "geojson"
      },
    "color": {"field": "Migrant Population", "type": "quantitative"},
    "tooltip": [
      {"field": "properties.STE_NAME16", "type": "nominal", "title": "State"},
      {"field": "Migrant Population", "type": "quantitative", "title": "Migrant Population"}
    ]
  },
  "config": {}
}