{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "description": "Unemployment rate over time",
  "background": "#FFFFFF",
  "padding": 5,
  "width": 500,
  "height": 400,
  "style": "cell",
  "data": [
    {
      "name": "hover_store",
      "values": [
        {
          "unit": "layer_0",
          "fields": [{"type": "E", "field": "Type"}],
          "values": ["Born overseas"]
        }
      ]
    },
    {
      "name": "source_0",
      "url": "https://raw.githubusercontent.com/ahmedbonyeamin/fit3179-asgn2/master/data/unemployment.csv",
      "format": {"type": "csv", "parse": {"Date": "date"}}
    },
    {
      "name": "data_0",
      "source": "source_0",
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["Date", "Type", "Rate"],
          "ops": ["argmax"],
          "fields": ["date"],
          "as": ["argmax_date"]
        },
        {
          "type": "filter",
          "expr": "(isDate(datum[\"Date\"]) || (isValid(datum[\"Date\"]) && isFinite(+datum[\"Date\"]))) && isValid(datum[\"argmax_date\"][\"Rate\"]) && isFinite(+datum[\"argmax_date\"][\"Rate\"])"
        }
      ]
    }
  ],
  "signals": [
    {
      "name": "unit",
      "value": {},
      "on": [
        {"events": "mousemove", "update": "isTuple(group()) ? group() : unit"}
      ]
    },
    {
      "name": "hover",
      "update": "vlSelectionResolve(\"hover_store\", \"union\", true, true)"
    },
    {
      "name": "hover_tuple",
      "on": [
        {
          "events": [{"source": "scope", "type": "mouseover"}],
          "update": "datum && item().mark.marktype !== 'group' ? {unit: \"layer_0\", fields: hover_tuple_fields, values: [(item().isVoronoi ? datum.datum : datum)[\"Type\"]]} : null",
          "force": true
        },
        {"events": [{"source": "view", "type": "dblclick"}], "update": "null"}
      ]
    },
    {"name": "hover_tuple_fields", "value": [{"type": "E", "field": "Type"}]},
    {
      "name": "hover_toggle",
      "value": false,
      "on": [
        {
          "events": [{"source": "scope", "type": "mouseover"}],
          "update": "event.shiftKey"
        },
        {"events": [{"source": "view", "type": "dblclick"}], "update": "false"}
      ]
    },
    {
      "name": "hover_modify",
      "on": [
        {
          "events": {"signal": "hover_tuple"},
          "update": "modify(\"hover_store\", hover_toggle ? null : hover_tuple, hover_toggle ? null : true, hover_toggle ? hover_tuple : null)"
        }
      ]
    }
  ],
  "marks": [
    {
      "name": "layer_0_pathgroup",
      "type": "group",
      "from": {
        "facet": {
          "name": "faceted_path_layer_0_main",
          "data": "source_0",
          "groupby": ["Type"]
        }
      },
      "encode": {
        "update": {
          "width": {"field": {"group": "width"}},
          "height": {"field": {"group": "height"}}
        }
      },
      "marks": [
        {
          "name": "layer_0_marks",
          "type": "line",
          "style": ["line"],
          "sort": {"field": "datum[\"Date\"]"},
          "interactive": true,
          "from": {"data": "faceted_path_layer_0_main"},
          "encode": {
            "update": {
              "stroke": {"value": "transparent"},
              "opacity": [
                {
                  "test": "!length(data(\"hover_store\")) || vlSelectionTest(\"hover_store\", datum)",
                  "value": 1
                },
                {"value": 0.2}
              ],
              "tooltip": {
                "signal": "{\"Type\": isValid(datum[\"Type\"]) ? datum[\"Type\"] : \"\"+datum[\"Type\"], \"Unemployment Rate\": format(datum[\"Rate\"], \"\")}"
              },
              "description": {
                "signal": "\"Date: \" + (timeFormat(datum[\"Date\"], '%b %d, %Y')) + \"; Unemployment rate: \" + (format(datum[\"Rate\"], \"\")) + \"; Type: \" + (isValid(datum[\"Type\"]) ? datum[\"Type\"] : \"\"+datum[\"Type\"]) + \"; Unemployment Rate: \" + (format(datum[\"Rate\"], \"\"))"
              },
              "x": {"scale": "x", "field": "Date"},
              "y": {"scale": "y", "field": "Rate"},
              "strokeWidth": {"value": 2},
              "defined": {
                "signal": "isValid(datum[\"Date\"]) && isFinite(+datum[\"Date\"]) && isValid(datum[\"Rate\"]) && isFinite(+datum[\"Rate\"])"
              }
            }
          }
        }
      ]
    },
    {
      "name": "layer_1_pathgroup",
      "type": "group",
      "from": {
        "facet": {
          "name": "faceted_path_layer_1_main",
          "data": "source_0",
          "groupby": ["Type"]
        }
      },
      "encode": {
        "update": {
          "width": {"field": {"group": "width"}},
          "height": {"field": {"group": "height"}}
        }
      },
      "marks": [
        {
          "name": "layer_1_marks",
          "type": "line",
          "style": ["line"],
          "sort": {"field": "datum[\"Date\"]"},
          "interactive": true,
          "from": {"data": "faceted_path_layer_1_main"},
          "encode": {
            "update": {
              "stroke": [
                {
                  "test": "!length(data(\"hover_store\")) || vlSelectionTest(\"hover_store\", datum)",
                  "scale": "color",
                  "field": "Type"
                },
                {"value": "grey"}
              ],
              "opacity": [
                {
                  "test": "!length(data(\"hover_store\")) || vlSelectionTest(\"hover_store\", datum)",
                  "value": 1
                },
                {"value": 0.2}
              ],
              "tooltip": {
                "signal": "{\"Type\": isValid(datum[\"Type\"]) ? datum[\"Type\"] : \"\"+datum[\"Type\"], \"Unemployment Rate\": format(datum[\"Rate\"], \"\")}"
              },
              "description": {
                "signal": "\"Date: \" + (timeFormat(datum[\"Date\"], '%b %d, %Y')) + \"; Unemployment rate: \" + (format(datum[\"Rate\"], \"\")) + \"; Type: \" + (isValid(datum[\"Type\"]) ? datum[\"Type\"] : \"\"+datum[\"Type\"]) + \"; Unemployment Rate: \" + (format(datum[\"Rate\"], \"\"))"
              },
              "x": {"scale": "x", "field": "Date"},
              "y": {"scale": "y", "field": "Rate"},
              "strokeWidth": {"value": 2},
              "defined": {
                "signal": "isValid(datum[\"Date\"]) && isFinite(+datum[\"Date\"]) && isValid(datum[\"Rate\"]) && isFinite(+datum[\"Rate\"])"
              }
            }
          }
        }
      ]
    },
    {
      "name": "layer_2_layer_0_marks",
      "type": "symbol",
      "style": ["circle"],
      "interactive": true,
      "from": {"data": "data_0"},
      "encode": {
        "update": {
          "fill": [
            {
              "test": "!length(data(\"hover_store\")) || vlSelectionTest(\"hover_store\", datum)",
              "scale": "color",
              "field": "Type"
            },
            {"value": "grey"}
          ],
          "opacity": [
            {
              "test": "!length(data(\"hover_store\")) || vlSelectionTest(\"hover_store\", datum)",
              "value": 1
            },
            {"value": 0.2}
          ],
          "tooltip": {
            "signal": "{\"Type\": isValid(datum[\"Type\"]) ? datum[\"Type\"] : \"\"+datum[\"Type\"], \"Unemployment Rate\": format(datum[\"Rate\"], \"\")}"
          },
          "ariaRoleDescription": {"value": "circle"},
          "description": {
            "signal": "\"Date: \" + (timeFormat(datum[\"Date\"], '%b %d, %Y')) + \"; Unemployment rate: \" + (format(datum[\"argmax_date\"][\"Rate\"], \"\")) + \"; Type: \" + (isValid(datum[\"Type\"]) ? datum[\"Type\"] : \"\"+datum[\"Type\"]) + \"; Unemployment Rate: \" + (format(datum[\"Rate\"], \"\"))"
          },
          "x": {"scale": "x", "field": "Date"},
          "y": {"scale": "y", "field": "argmax_date[\"Rate\"]"},
          "shape": {"value": "circle"}
        }
      }
    },
    {
      "name": "layer_2_layer_1_marks",
      "type": "text",
      "style": ["text"],
      "interactive": true,
      "from": {"data": "data_0"},
      "encode": {
        "update": {
          "align": {"value": "left"},
          "dx": {"value": 4},
          "fill": [
            {
              "test": "!length(data(\"hover_store\")) || vlSelectionTest(\"hover_store\", datum)",
              "scale": "color",
              "field": "Type"
            },
            {"value": "grey"}
          ],
          "opacity": [
            {
              "test": "!length(data(\"hover_store\")) || vlSelectionTest(\"hover_store\", datum)",
              "value": 1
            },
            {"value": 0.2}
          ],
          "tooltip": {
            "signal": "{\"Type\": isValid(datum[\"Type\"]) ? datum[\"Type\"] : \"\"+datum[\"Type\"], \"Unemployment Rate\": format(datum[\"Rate\"], \"\")}"
          },
          "description": {
            "signal": "\"Date: \" + (timeFormat(datum[\"Date\"], '%b %d, %Y')) + \"; Unemployment rate: \" + (format(datum[\"argmax_date\"][\"Rate\"], \"\")) + \"; Type: \" + (isValid(datum[\"Type\"]) ? datum[\"Type\"] : \"\"+datum[\"Type\"]) + \"; Unemployment Rate: \" + (format(datum[\"Rate\"], \"\"))"
          },
          "x": {"scale": "x", "field": "Date"},
          "y": {"scale": "y", "field": "argmax_date[\"Rate\"]"},
          "text": {
            "signal": "isValid(datum[\"Type\"]) ? datum[\"Type\"] : \"\"+datum[\"Type\"]"
          },
          "fontSize": {"value": 11},
          "baseline": {"value": "middle"}
        }
      }
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "time",
      "domain": {
        "fields": [
          {"data": "source_0", "field": "Date"},
          {"data": "data_0", "field": "Date"}
        ]
      },
      "range": [0, {"signal": "width"}]
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "fields": [
          {"data": "source_0", "field": "Rate"},
          {"data": "data_0", "field": "argmax_date[\"Rate\"]"}
        ]
      },
      "range": [{"signal": "height"}, 0],
      "nice": true,
      "zero": true
    },
    {
      "name": "color",
      "type": "ordinal",
      "domain": {
        "fields": [
          {"data": "source_0", "field": "Type"},
          {"data": "data_0", "field": "Type"}
        ],
        "sort": true
      },
      "range": "category"
    }
  ],
  "axes": [
    {
      "scale": "y",
      "orient": "left",
      "gridScale": "x",
      "grid": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "domain": false,
      "labels": false,
      "aria": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "x",
      "orient": "bottom",
      "grid": false,
      "title": "Date",
      "labelBaseline": "top",
      "labelFlush": true,
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(width/40)"},
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "grid": false,
      "title": "Unemployment rate",
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "zindex": 0
    }
  ],
  "legends": [
    {
      "stroke": "color",
      "symbolType": "circle",
      "title": "Type",
      "encode": {
        "symbols": {
          "update": {"strokeWidth": {"value": 2}, "opacity": {"value": 1}}
        }
      },
      "fill": "color"
    }
  ],
  "config": {
    "range": {
      "category": [
        "#1696d2",
        "#ec008b",
        "#fdbf11",
        "#000000",
        "#d2d2d2",
        "#55b748"
      ],
      "diverging": [
        "#ca5800",
        "#fdbf11",
        "#fdd870",
        "#fff2cf",
        "#cfe8f3",
        "#73bfe2",
        "#1696d2",
        "#0a4c6a"
      ],
      "heatmap": [
        "#ca5800",
        "#fdbf11",
        "#fdd870",
        "#fff2cf",
        "#cfe8f3",
        "#73bfe2",
        "#1696d2",
        "#0a4c6a"
      ],
      "ordinal": [
        "#cfe8f3",
        "#a2d4ec",
        "#73bfe2",
        "#46abdb",
        "#1696d2",
        "#12719e"
      ],
      "ramp": [
        "#CFE8F3",
        "#A2D4EC",
        "#73BFE2",
        "#46ABDB",
        "#1696D2",
        "#12719E",
        "#0A4C6A",
        "#062635"
      ]
    },
    "path": {"stroke": "#1696d2", "strokeWidth": 0.5},
    "shape": {"stroke": "#1696d2"},
    "symbol": {"fill": "#1696d2", "size": 30},
    "axisX": {
      "domain": true,
      "domainColor": "#000000",
      "domainWidth": 1,
      "grid": false,
      "labelFontSize": 12,
      "labelFont": "Lato",
      "labelAngle": 0,
      "tickColor": "#000000",
      "tickSize": 5,
      "titleFontSize": 12,
      "titlePadding": 10,
      "titleFont": "Lato"
    },
    "axisY": {
      "domain": false,
      "domainWidth": 1,
      "grid": true,
      "gridColor": "#DEDDDD",
      "gridWidth": 1,
      "labelFontSize": 12,
      "labelFont": "Lato",
      "labelPadding": 8,
      "ticks": false,
      "titleFontSize": 12,
      "titlePadding": 10,
      "titleFont": "Lato",
      "titleAngle": 0,
      "titleY": -10,
      "titleX": 18
    },
    "legend": {
      "labelFontSize": 12,
      "labelFont": "Lato",
      "symbolSize": 100,
      "titleFontSize": 12,
      "titlePadding": 10,
      "titleFont": "Lato",
      "orient": "right",
      "offset": 10
    },
    "style": {
      "bar": {"fill": "#1696d2", "stroke": null},
      "cell": {"stroke": null},
      "arc": {"fill": "#1696d2"},
      "area": {"fill": "#1696d2"},
      "line": {"stroke": "#1696d2", "strokeWidth": 2},
      "rect": {"fill": "#1696d2"},
      "text": {
        "font": "Lato",
        "fontSize": 11,
        "align": "center",
        "fontWeight": 400,
        "size": 11
      },
      "trail": {"stroke": "#1696d2", "strokeWidth": 0, "size": 1},
      "group-title": {"fontSize": 18, "font": "Lato"}
    }
  }
}